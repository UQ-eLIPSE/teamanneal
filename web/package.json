{
  "name": "teamanneal",
  "version": "2.3.0",
  "description": "A web-based simulated annealing service for forming teams",
  "homepage": "https://www.elipse.uq.edu.au/",
  "author": "eLIPSE",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/build/server/src/index.js",
    "test": "npm run test_server",
    "test_server": "jest --no-cache --config=test/server.jest.config.json",
    "clean": "npm run clean_build",
    "clean_build": "node ./scripts/CleanBuild.js",
    "build": "npm run clean && npm run build_client && npm run build_server",
    "build_prod": "npm run clean && npm run build_client_prod && npm run build_server",
    "build_client": "cd ./client && webpack --config webpack.config.js && npm run build_client_copy_index",
    "build_client_prod": "cd ./client && webpack -p --config webpack.config.js && npm run build_client_copy_index",
    "build_client_copy_index": "node ./scripts/CopyClientToBuild.js",
    "build_client_watch": "cd ./client && npm run build_client_copy_index && webpack --watch --config webpack.config.js",
    "build_server": "tsc --project ./server",
    "build_server_watch": "tsc --project ./server --watch"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/express": "4.11.1",
    "@types/file-saver": "1.3.0",
    "@types/fs-extra": "5.0.1",
    "@types/jest": "22.2.0",
    "@types/kue": "0.11.8",
    "@types/node": "9.4.7",
    "@types/papaparse": "4.1.33",
    "@types/uuid": "3.4.3",
    "@types/winston": "2.3.8",
    "av-ts": "0.9.2",
    "css-loader": "0.28.10",
    "file-loader": "1.1.11",
    "image-webpack-loader": "4.1.0",
    "jest": "19.0.2",
    "style-loader": "0.20.2",
    "ts-jest": "19.0.14",
    "ts-loader": "4.0.1",
    "typescript": "2.9.1",
    "vue-loader": "14.2.1",
    "vue-template-compiler": "2.5.13",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.10"
  },
  "dependencies": {
    "@types/redis": "2.8.5",
    "axios": "0.16.2",
    "body-parser": "1.17.1",
    "chart.js": "2.7.2",
    "es6-promise": "4.1.0",
    "express": "4.15.2",
    "file-saver": "1.3.3",
    "fs-extra": "2.1.2",
    "kue": "0.11.6",
    "papaparse": "4.3.2",
    "uuid": "3.1.0",
    "vue": "2.5.13",
    "vue-chartjs": "3.4.0",
    "vue-router": "3.0.1",
    "vuex": "3.0.1",
    "winston": "2.3.1",
    "winston-daily-rotate-file": "1.4.6"
  }
}
