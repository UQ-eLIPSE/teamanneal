{
  "name": "teamanneal",
  "version": "2.3.0",
  "description": "A web-based simulated annealing service for forming teams",
  "homepage": "https://www.elipse.uq.edu.au/",
  "author": "eLIPSE",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node --turbo --ignition ./build/server/src/index.js",
    "test": "npm run test_server",
    "test_server": "jest --no-cache --config=test/server.jest.config.json",
    "clean": "npm run clean_build",
    "clean_build": "node ./scripts/CleanBuild.js",
    "build": "npm run clean && npm run build_client && npm run build_server",
    "build_prod": "npm run clean && npm run build_client_prod && npm run build_server",
    "build_client": "cd ./client && webpack --config webpack.config.js && npm run build_client_copy_index",
    "build_client_prod": "cd ./client && webpack -p --config webpack.config.js && npm run build_client_copy_index",
    "build_client_copy_index": "node ./scripts/CopyClientToBuild.js",
    "build_server": "tsc --project ./server"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.3",
    "@types/express": "4.0.36",
    "@types/file-saver": "0.0.1",
    "@types/fs-extra": "2.1.0",
    "@types/jest": "19.2.4",
    "@types/kue": "0.11.4",
    "@types/node": "7.0.31",
    "@types/papaparse": "4.1.29",
    "@types/uuid": "3.0.0",
    "@types/winston": "2.3.1",
    "av-ts": "0.9.0",
    "css-loader": "0.28.4",
    "file-loader": "0.11.2",
    "image-webpack-loader": "3.3.1",
    "jest": "19.0.2",
    "style-loader": "0.18.2",
    "ts-jest": "19.0.14",
    "ts-loader": "2.1.0",
    "typescript": "2.7.2",
    "vue-loader": "12.2.1",
    "vue-template-compiler": "2.3.3",
    "webpack": "2.6.1"
  },
  "dependencies": {
    "axios": "0.16.2",
    "body-parser": "1.17.1",
    "es6-promise": "4.1.0",
    "express": "4.15.2",
    "file-saver": "1.3.3",
    "fs-extra": "2.1.2",
    "kue": "0.11.6",
    "papaparse": "4.3.2",
    "uuid": "3.1.0",
    "vue": "2.3.3",
    "vue-router": "2.5.3",
    "vuex": "2.3.1",
    "winston": "2.3.1",
    "winston-daily-rotate-file": "1.4.6"
  }
}
